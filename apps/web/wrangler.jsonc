{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "shipcru",
  "routes": [
    { "pattern": "shipcru.com", "custom_domain": true },
    { "pattern": "www.shipcru.com", "custom_domain": true }
  ],
  "compatibility_date": "2026-02-25",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "main": "./worker/index.ts",
  "assets": {
    "not_found_handling": "none",
    "binding": "ASSETS"
  },
  "images": {
    "binding": "IMAGES"
  }
}
